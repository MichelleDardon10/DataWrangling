# Filtrar los territorios y calcular la suma de ventas netas
territorios_perdidas <- parcial_anonimo %>%
group_by(Territorio) %>%
summarise(Venta_neta = sum(as.numeric(Venta))) %>%
filter(Venta_neta < 0) %>%         # Filtrar solo los territorios con ventas netas negativas
arrange(Venta_neta)                # Ordenar los territorios por pérdidas más grandes (más negativas)
# Mostrar el resultado
territorios_perdidas
